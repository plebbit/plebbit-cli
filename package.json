{
    "name": "plebbit-cli",
    "version": "0.9.2",
    "description": "Command line interface to Plebbit API",
    "types": "./dist/index.d.ts",
    "homepage": "https://github.com/plebbit/plebbit-cli",
    "repository": "git@github.com:plebbit/plebbit-cli.git",
    "author": "Esteban Abaroa, Rinse",
    "license": "GPL-2.0-only",
    "type": "commonjs",
    "private": true,
    "exports": null,
    "scripts": {
        "build": "rm -rf dist/  && tsc --project config/tsconfig.json",
        "build:watch": "nodemon --config config/build.nodemon.json",
        "test:cli": " ts-mocha --config config/.mocharc.json -p config/tsconfig.json test/cli/*.test.ts",
        "commit": "cz",
        "prepare": "husky install",
        "release": "HUSKY=0 release-it --config config/.release-it.json"
    },
    "files": [
        "/bin",
        "/dist",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "bin": {
        "plebbit": "./bin/run"
    },
    "main": "./dist/index.js",
    "oclif": {
        "bin": "plebbit",
        "dirname": "plebbit",
        "commands": "./dist/cli/commands",
        "topicSeparator": " ",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-not-found"
        ],
        "topics": {
            "subplebbit": {
                "description": "Access and manage your subplebbits"
            },
            "subplebbit:role": {
                "description": "Manage the role of authors in your subplebbits"
            }
        },
        "hooks": {
            "prerun": [
                "./dist/cli/hooks/prerun/parse-dynamic-flags-hook"
            ]
        }
    },
    "pkg": {
        "scripts": "./dist/**/*.js",
        "assets": [
            "node_modules/{swagger-ui-dist, sqlite3, skia-canvas, @keyv/sqlite}/**/*",
            "node_modules/**/*sqlite3.node",
            "node_modules/sqlite3/lib/**/node_sqlite3.node",
            "node_modules/**/skia-canvas/lib/v6/index.node",
            "node_modules/skia-canvas/lib/v6/index.node",
            "node_modules/captcha-canvas/node_modules/skia-canvas/lib/v6/index.node",
            "node_modules/skia-canvas/lib/**/*.*",
            "node_modules/skia-canvas/**/*.*"
        ]
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "engines": {
        "node": ">=16.0.0 <17.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "17.3.0",
        "@commitlint/config-conventional": "17.3.0",
        "@oclif/test": "2.4.5",
        "@release-it/conventional-changelog": "5.1.1",
        "@tsconfig/node16-strictest": "1.0.4",
        "@types/chai": "4.3.4",
        "@types/fs-extra": "9.0.13",
        "@types/lodash": "4.14.189",
        "@types/mocha": "10.0.0",
        "@types/node": "16.18.1",
        "@types/prettier": "2.7.1",
        "chai": "4.3.7",
        "chai-as-promised": "7.1.1",
        "commitizen": "4.2.5",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "8.27.0",
        "eslint-config-oclif": "4.0.0",
        "eslint-config-oclif-typescript": "1.0.3",
        "husky": "8.0.2",
        "mocha": "10.1.0",
        "nodemon": "2.0.20",
        "oclif": "3.11.3",
        "pkg": "5.8.1",
        "release-it": "15.5.1",
        "shx": "0.3.4",
        "sinon": "17.0.1",
        "tempy": "1.0.1",
        "ts-mocha": "10.0.0",
        "ts-node": "10.9.1",
        "wait-on": "6.0.1"
    },
    "dependencies": {
        "@oclif/core": "2.11.8",
        "@oclif/plugin-help": "5.2.17",
        "@oclif/plugin-not-found": "2.3.37",
        "@plebbit/plebbit-js": "https://github.com/plebbit/plebbit-js#e7e40aff4d7e806da9f754a7f7944c032f01b70c",
        "dataobject-parser": "1.2.22",
        "env-paths": "2.2.1",
        "is-ipfs": "7.0.3",
        "kubo": "0.24.0",
        "lodash-es": "4.17.21",
        "skia-canvas": "1.0.0",
        "tslib": "2.6.1",
        "typescript": "5.1.6"
    },
    "resolutions": {
        "skia-canvas": "1.0.0",
        "@octokit/request-error": "3.0.3",
        "@multiformats/multiaddr": "12.1.3"
    }
}
